{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "http://localhost:3000/schemas/streaming_history.json",
    "title": "Streaming History Audio",
    "description": "History of every song played on Spotify",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "ts": { "type": "string" },
            "ms_played": { "type": "integer" },
            "spotify_track_uri": { "type": ["string", "null"] }
        },
        "optionalProperties": {
            "username": { "type": "string" },
            "platform": { "type": "string" },
            "conn_country": { "type": "string" },
            "ip_addr_decrypted": { "type": "string" },
            "user_agent_decrypted": { "type": ["string", "null"] },
            "master_metadata_track_name": { "type": ["string", "null"] },
            "master_metadata_album_artist_name": { "type": ["string", "null"] },
            "master_metadata_album_album_name": { "type": ["string", "null"] },
            "episode_name": { "type": ["string", "null"] },
            "episode_show_name": { "type": ["string", "null"] },
            "episode_show_uri": { "type": ["string", "null"] },
            "reason_start": { "type": "string" },
            "reason_end": { "type": "string" },
            "shuffle": { "type": "boolean" },
            "skipped": { "type": ["boolean", "null"] },
            "offline": { "type": "boolean" },
            "offline_timestamp": { "type": "integer" },
            "incognito_mode": { "type": "boolean" }
        },
        "required": ["ts", "ms_played", "spotify_track_uri"],
        "additionalProperties": false
    },
    "minItems": 1
}